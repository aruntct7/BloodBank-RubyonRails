    <%= form_for :comment ,url: {action: "create"} do |f| %>


        <%= f.text_field :name,:size=>30,:maxlength=>15,:placeholder=>'FullName' %>
        <br>

        <%= f.text_field :email_id,:size=>30,:maxlength=>50,:placeholder=>'Email ID' %>
